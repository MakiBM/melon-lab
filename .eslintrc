{
  "env": {
    "jest": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": ["**/*.test.js", "**/*.story.jsx", "/tests/**"]
      }
    ]
  }
}
